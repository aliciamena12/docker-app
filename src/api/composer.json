{
    "require": {
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\Docs\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\Docs\\": "test/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}